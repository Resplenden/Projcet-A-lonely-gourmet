<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="edu.study.mapper.kakaoMapper">
 	<select id="kakao" parameterType="java.util.HashMap" resultType="kakaVo">
 	SELECT *
 	from kakaotb
 	WHERE nickname=#{nickname}
 	AND email=#{email} 	
 	</select>
	
	<insert id="insert" parameterType="java.util.HashMap">	
	INSERT INTO
	kakaotb(nickname,email)
	VALUES(#{nickname},#{email})	
	</insert> 
	 
</mapper>